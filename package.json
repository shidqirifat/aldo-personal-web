{
  "name": "aldo-personal-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format:all": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|jsx|tsx)\"",
    "format:dirty": "pretty-quick --ignore-path .gitignore --pattern \"**/*.+(js|ts|jsx|tsx)\"",
    "format:check": "prettier --ignore-path .gitignore --check \"**/*.+(js|ts|jsx|tsx)\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fontsource/inter": "^4.5.15",
    "@fontsource/poppins": "^4.5.10",
    "next": "13.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "eslint": "^8.39.0",
    "eslint-config-next": "^13.3.1",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "tailwindcss": "^3.3.2"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --quiet --fix"
    ]
  }
}
